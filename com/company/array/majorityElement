package array;
public class majorityElement { 
    static int major(int [] arr, int n){
        int cnt=0;
        for(int i=0;i<n;i++){
            cnt=0;
            for(int j=0;j<n;j++){
                if(arr[i]==arr[j]){
                    cnt++;
                }
            }
        }
    }
}